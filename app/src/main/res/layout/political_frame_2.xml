<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameNew"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/rounded_corners">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:background="@color/white"
            android:orientation="vertical"
            android:layout_gravity="bottom">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="#51221a"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="15dp"
                android:background="#f27f16"
                android:layout_gravity="bottom"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLogo"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="bottom"
            android:gravity="bottom"
            android:padding="1dp"
            android:visibility="visible">



            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tvName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:text="Name"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_13sdp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tvDesignation1"
                    android:layout_width="wrap_content"
                    android:textColor="@color/white"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:text="Designation1"
                    android:textSize="@dimen/_7sdp" />

                <TextView
                    android:id="@+id/tvDesignation2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/white"
                    android:layout_gravity="center_horizontal"
                    android:text="Designation2"
                    android:textSize="@dimen/_7sdp" />

                <TextView
                    android:id="@+id/tvMobileNo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:visibility="gone"
                    android:text="Mobile Number"
                    android:textSize="@dimen/_7sdp" />

                <ImageView
                    android:id="@+id/socialMediaIv"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/_8sdp"
                    android:layout_gravity="bottom"
                    android:layout_marginBottom="2dp"
                    android:layout_marginTop="6dp"
                    android:elevation="1dp"
                    android:src="@drawable/ic_all_social"
                    android:visibility="visible" />
            </LinearLayout>
            <ImageView
                android:id="@+id/iv_logo"
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_90sdp"
                android:layout_gravity="bottom"
                android:adjustViewBounds="true"
                android:scaleType="centerCrop"
                android:src="@mipmap/ic_launcher_foreground" />

        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp">


        <LinearLayout
            android:id="@+id/linearLogoL"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginLeft="@dimen/_5sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            android:orientation="horizontal"
            android:visibility="visible">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="1dp"
                android:orientation="horizontal"
                android:gravity="top"
                android:visibility="visible"
                android:weightSum="10">

                <ImageView
                    android:id="@+id/iv_party"

                    android:layout_width="@dimen/_70sdp"
                    android:layout_height="@dimen/_70sdp"
                    android:layout_gravity="bottom"
                    android:adjustViewBounds="true"
                    android:scaleType="centerCrop"
                    android:src="@mipmap/ic_launcher_foreground"
                    android:visibility="visible" />


                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="10" />


                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_1sdp"
                    android:background="@drawable/rounded_corners"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="4dp">

                    <ImageView
                        android:id="@+id/iv_logoL1"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:adjustViewBounds="true"
                        android:scaleType="centerCrop"
                        android:src="@drawable/demo_img"
                        android:visibility="visible" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_1sdp"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="4dp">

                    <ImageView
                        android:id="@+id/iv_logoL2"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:adjustViewBounds="true"
                        android:background="@drawable/rounded_corners"
                        android:scaleType="centerCrop"
                        android:src="@drawable/demo_img"
                        android:visibility="visible" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_1sdp"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="4dp">

                    <ImageView
                        android:id="@+id/iv_logoL3"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:adjustViewBounds="true"
                        android:background="@drawable/rounded_corners"
                        android:scaleType="centerCrop"
                        android:src="@drawable/demo_img"
                        android:visibility="visible" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_1sdp"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="4dp">

                    <ImageView
                        android:id="@+id/iv_logoL4"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:adjustViewBounds="true"
                        android:background="@drawable/rounded_corners"
                        android:scaleType="centerCrop"
                        android:src="@drawable/demo_img"
                        android:visibility="visible" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_1sdp"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="4dp">

                    <ImageView
                        android:id="@+id/iv_logoL5"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:adjustViewBounds="true"
                        android:background="@drawable/rounded_corners"
                        android:scaleType="centerCrop"
                        android:src="@drawable/demo_img"
                        android:visibility="visible" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_1sdp"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="4dp">

                    <ImageView
                        android:id="@+id/iv_logoL6"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:adjustViewBounds="true"
                        android:background="@drawable/rounded_corners"
                        android:scaleType="centerCrop"
                        android:src="@drawable/demo_img"
                        android:visibility="visible" />
                </androidx.cardview.widget.CardView>
            </LinearLayout>


        </LinearLayout>

    </FrameLayout>

</FrameLayout>