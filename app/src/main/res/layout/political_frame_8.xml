<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameNew"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="15dp"
        android:layout_gravity="bottom|end">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@color/white"

            android:layout_gravity="bottom">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:layout_gravity="bottom"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLogo"
            android:layout_width="match_parent"
            android:layout_height="65dp"
            android:background="@drawable/pf8background"
            android:gravity="bottom"
            android:layout_marginBottom="8dp"
            android:layout_gravity="bottom"
            android:padding="1dp"
            android:visibility="visible">


            <View
                android:layout_width="65dp"
                android:layout_height="1dp"/>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_weight="1"
                android:layout_marginLeft="-15dp"
                android:layout_marginBottom="5dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tvName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:textStyle="bold"
                    android:textColor="@color/white"
                    android:text="Name"
                    android:fontFamily="@font/khand_bold"
                    android:textSize="@dimen/_20sdp" />

                <TextView
                    android:id="@+id/tvDesignation1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/white"
                    android:fontFamily="@font/baloo"
                    android:layout_gravity="start"
                    android:layout_marginLeft="12dp"
                    android:text="Designation1"
                    android:textSize="@dimen/_9sdp" />

                <TextView
                    android:id="@+id/tvDesignation2"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_gravity="center_horizontal"
                    android:text="Designation2"
                    android:textSize="@dimen/_7sdp" />
                <TextView
                    android:id="@+id/tvMobileNo"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_gravity="center_horizontal"

                    android:text="Mobile Number"
                    android:textSize="@dimen/_7sdp" />
                <ImageView
                    android:id="@+id/socialMediaIv"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/_8sdp"
                    android:layout_gravity="center_horizontal"
                    android:elevation="1dp"
                    android:src="@drawable/ic_all_social"
                    android:visibility="gone" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_weight="1"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="bottom"
            android:gravity="bottom">

            <View
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="1dp"/>
            <ImageView
                android:id="@+id/iv_logo"
                android:layout_width="@dimen/_100sdp"
                android:layout_height="@dimen/_100sdp"
                android:layout_gravity="bottom|right"
                android:adjustViewBounds="true"
                android:scaleType="fitCenter"
                android:src="@drawable/p5profile"
                android:visibility="visible" />
        </LinearLayout>



    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp">


        <LinearLayout
            android:id="@+id/linearLogoL"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="top"
            android:layout_marginLeft="@dimen/_5sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            android:orientation="horizontal"
            android:visibility="visible">
            <ImageView
                android:id="@+id/iv_party"
                android:layout_width="@dimen/_50sdp"
                android:layout_height="@dimen/_50sdp"
                android:layout_gravity="start|top"
                android:adjustViewBounds="true"
                android:scaleType="centerCrop"
                android:src="@mipmap/ic_launcher_foreground"
                android:visibility="visible" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="1dp"
                android:orientation="horizontal"
                android:gravity="top"
                android:visibility="visible"
                android:weightSum="10">




                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="10" />


                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_1sdp"
                    app:cardElevation="4dp">

                    <ImageView
                        android:id="@+id/iv_logoL1"
                        android:layout_width="@dimen/_21sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:adjustViewBounds="true"
                        android:scaleType="centerCrop"
                        android:src="@drawable/pf5leader"
                        android:visibility="visible" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_1sdp"
                    app:cardElevation="4dp">

                    <ImageView
                        android:id="@+id/iv_logoL2"
                        android:layout_width="@dimen/_21sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:adjustViewBounds="true"
                        android:scaleType="centerCrop"
                        android:src="@drawable/pf5leader"
                        android:visibility="visible" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_1sdp"
                    app:cardElevation="4dp">

                    <ImageView
                        android:id="@+id/iv_logoL3"
                        android:layout_width="@dimen/_21sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:adjustViewBounds="true"
                        android:scaleType="centerCrop"
                        android:src="@drawable/pf5leader"
                        android:visibility="visible" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_1sdp"
                    app:cardElevation="4dp">

                    <ImageView
                        android:id="@+id/iv_logoL4"
                        android:layout_width="@dimen/_21sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:adjustViewBounds="true"
                        android:scaleType="centerCrop"
                        android:src="@drawable/pf5leader"
                        android:visibility="visible" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_1sdp"
                    app:cardElevation="4dp">

                    <ImageView
                        android:id="@+id/iv_logoL5"
                        android:layout_width="@dimen/_21sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:adjustViewBounds="true"
                        android:scaleType="centerCrop"
                        android:src="@drawable/pf5leader"
                        android:visibility="visible" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_1sdp"
                    app:cardElevation="4dp">

                    <ImageView
                        android:id="@+id/iv_logoL6"
                        android:layout_width="@dimen/_21sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:adjustViewBounds="true"
                        android:scaleType="centerCrop"
                        android:src="@drawable/pf5leader"
                        android:visibility="visible" />
                </androidx.cardview.widget.CardView>
            </LinearLayout>


        </LinearLayout>

    </FrameLayout>

</FrameLayout>